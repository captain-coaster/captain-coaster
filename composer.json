{
    "name": "captain-coaster/captain-coaster",
    "license": "MIT",
    "type": "project",
    "description": "Participative guide to discover, rate and review roller coasters",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-json": "*",
        "api-platform/core": "^4.0",
        "aws/aws-sdk-php": "^3.0",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^3.3",
        "easycorp/easyadmin-bundle": "^4",
        "excelwebzone/recaptcha-bundle": "^1.5",
        "friendsofsymfony/jsrouting-bundle": "^3.0",
        "http-interop/http-factory-guzzle": "^1.2.0",
        "knplabs/knp-paginator-bundle": "^6",
        "knplabs/knp-time-bundle": "^2",
        "knpuniversity/oauth2-client-bundle": "^2.15",
        "league/flysystem-aws-s3-v3": "^3",
        "league/oauth2-google": "^4",
        "oneup/flysystem-bundle": "^4",
        "stof/doctrine-extensions-bundle": "^1.14",
        "symfony/amazon-mailer": "7.*",
        "symfony/asset": "7.*",
        "symfony/cache": "7.*",
        "symfony/discord-notifier": "7.*",
        "symfony/dom-crawler": "7.*",
        "symfony/dotenv": "7.*",
        "symfony/expression-language": "7.*",
        "symfony/flex": "^2.0",
        "symfony/form": "7.*",
        "symfony/http-client": "7.*",
        "symfony/lock": "7.*",
        "symfony/mailer": "7.*",
        "symfony/messenger": "7.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.*",
        "symfony/rate-limiter": "7.*",
        "symfony/redis-messenger": "7.*",
        "symfony/runtime": "7.*",
        "symfony/security-bundle": "7.*",
        "symfony/stimulus-bundle": "^2.23",
        "symfony/stopwatch": "7.*",
        "symfony/translation": "7.*",
        "symfony/twig-bundle": "7.*",
        "symfony/validator": "7.*",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfonycasts/verify-email-bundle": "^1.16",
        "twig/cssinliner-extra": "^3.8",
        "twig/extra-bundle": "^3.8",
        "twig/inky-extra": "^3.8",
        "twig/intl-extra": "^3.8",
        "symfony/ux-translator": "^2.13"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.88.2",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.3",
        "phpunit/phpunit": "^11.0",
        "symfony/browser-kit": "7.*",
        "symfony/css-selector": "7.*",
        "symfony/debug-bundle": "7.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "7.*",
        "symfony/web-profiler-bundle": "7.*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        },
        "platform": {
            "php": "8.4.13"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpstan": "vendor/bin/phpstan"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.*"
        }
    }
}
